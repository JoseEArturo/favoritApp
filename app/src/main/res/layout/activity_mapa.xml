<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Mapa">

    <com.google.android.gms.maps.MapView
        android:id="@+id/mapa_fondo"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:layout_editor_absoluteX="-205dp"
        tools:layout_editor_absoluteY="-72dp" />

    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/mapa_btnRegresar"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="308dp"
        android:layout_marginBottom="76dp"
        android:clickable="true"
        android:src="@drawable/ic_regresar"
        app:backgroundTint="#0D8DB5"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />

    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/mapa_btnAgregar"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="308dp"
        android:layout_marginBottom="76dp"
        android:clickable="true"
        android:src="@drawable/ic_nuevo"
        app:backgroundTint="#24B8AB"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:rippleColor="@color/cardview_dark_background" />

</androidx.constraintlayout.widget.ConstraintLayout>